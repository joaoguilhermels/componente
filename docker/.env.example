# Docker environment variables for Customer Registry
# Copy this file to .env and adjust the values for your environment.
#
# IMPORTANT: Never commit .env to version control.

# PostgreSQL
POSTGRES_DB=customer_registry
POSTGRES_USER=cruser
POSTGRES_PASSWORD=changeme

# pgAdmin
PGADMIN_DEFAULT_EMAIL=admin@1ff.com
PGADMIN_DEFAULT_PASSWORD=changeme

# Spring Boot datasource (must match PostgreSQL credentials)
SPRING_DATASOURCE_URL=jdbc:postgresql://postgres:5432/customer_registry
SPRING_DATASOURCE_USERNAME=cruser
SPRING_DATASOURCE_PASSWORD=changeme
