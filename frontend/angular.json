{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "customer-registry-ui": {
      "projectType": "library",
      "root": "projects/customer-registry-ui",
      "sourceRoot": "projects/customer-registry-ui/src",
      "prefix": "crui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/customer-registry-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/customer-registry-ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/customer-registry-ui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "example-crm-app": {
      "projectType": "application",
      "root": "projects/example-crm-app",
      "sourceRoot": "projects/example-crm-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/example-crm-app",
            "index": "projects/example-crm-app/src/index.html",
            "browser": "projects/example-crm-app/src/main.ts",
            "tsConfig": "projects/example-crm-app/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "example-crm-app:build:production"
            },
            "development": {
              "buildTarget": "example-crm-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  }
}
